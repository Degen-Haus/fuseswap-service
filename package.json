{
  "name": "node-skeleton",
  "version": "0.1.0",
  "description": "node-skeleton",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./src/app.ts",
    "debug": "node --inspect-brk --trace-warnings ./src/app.js",
    "start-prod": "node ./src/app.js",
    "doc": "apidoc -i src -o docs/api && apidoc-markdown -p docs/api -o docs/api.md",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/node-skeleton.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "assert"
    ],
    "ignore": [
      "docs"
    ]
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "@ethersproject/abi": "^5.0.9",
    "@ethersproject/address": "^5.0.8",
    "@ethersproject/contracts": "^5.0.8",
    "@ethersproject/providers": "^5.0.17",
    "@ethersproject/solidity": "^5.0.7",
    "@ethersproject/units": "^5.0.8",
    "@fuseio/fuse-swap-default-token-list": "^3.0.0",
    "@fuseio/fuse-swap-sdk": "^3.1.1",
    "@types/config": "0.0.37",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/express-jwt": "0.0.42",
    "@types/jest": "^26.0.19",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.13",
    "@uniswap/token-lists": "^1.0.0-beta.19",
    "async": "^2.6.1",
    "aws-sdk": "^2.405.0",
    "body-parser": "^1.18.3",
    "config": "^1.30.0",
    "cors": "2.7.1",
    "debug": "2.6.9",
    "eslint-config-prettier": "^7.0.0",
    "express": "4.16.3",
    "express-async-errors": "^2.1.2",
    "express-jwt": "^5.3.1",
    "express-paginate": "^1.0.0",
    "jest": "^26.6.3",
    "jsonwebtoken": "^8.4.0",
    "lodash.flatmap": "^4.5.0",
    "lodash.uniq": "^4.5.0",
    "methods": "1.1.2",
    "module-alias": "^2.1.0",
    "moment": "^2.23.0",
    "mongoose": "^5.7.5",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "validator": "^10.10.0"
  },
  "devDependencies": {
    "@rigwild/apidoc-markdown": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "apidoc": "^0.17.7",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "nodemon": "^1.18.11",
    "prettier": "^2.2.1",
    "standard": "^12.0.1"
  }
}
